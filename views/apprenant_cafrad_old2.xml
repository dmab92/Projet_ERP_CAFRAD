<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="view_apprenant_cafrad_filter" model="ir.ui.view">
            <field name="name">apprenant.cafrad.filter</field>
            <field name="model">apprenant.cafrad</field>
            <field name="arch" type="xml">
                <search string="Les Apprenants">
                    <field name="name"/>
                    <field name="matricule" />
                    <field name="classe_id" />
                    <field name="school"/>
                    <group expand="0" string="Group By">
                         <filter string="Anne Academique" name="groupby_ane_academique_id" domain="[]" context="{'group_by': 'ane_academique_id'}"/>
                        <filter string="Etablissement" name="groupby_school" domain="[]" context="{'group_by': 'school'}"/>
                        <filter string="Salle de classe" name="groupby_classe_id" domain="[]" context="{'group_by': 'classe_id'}"/>
                         <filter string="mobility" name="groupby_mobility" domain="[]" context="{'group_by': 'mobility'}"/>
                    </group>
                </search>
            </field>
        </record>

        <record model="ir.ui.view" id="view_apprenant_cafrad_tree" >
            <field name="name">Liste des Apprenants</field>
            <field name="model">apprenant.cafrad</field>
            <field name="arch" type="xml">
                <tree string="Liste des Apprenants" >
                    <field name="matricule"/>
                    <field name="name"/>
                    <field name="school"/>
                    <field name="classe_id" />
                    <field name="ane_academique_id"/>
                    <field name="sexe"/>
                    <field name="mobility"/>
                    <field name="age"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="view_apprenant_cafrad_form">
            <field name="name">Apprenants</field>
            <field name="model">apprenant.cafrad</field>
            <field name="arch" type="xml">
                <form string="Apprenants">
                        <p class="oe_grey">
                                    Cr&#233;ez / &#233;ditez vos donn&#233;es sur les apprenants
                        </p>
                    <header>
                        <button name="button_validate"
                                string="Faire passer le prospect en Apprenant/Client"
                                type="object"
                                class="oe_highlight"
                                icon="fa-check-circle"
                                attrs="{'invisible': [('state','=', 'student')]}"/>

                        <field name="state" widget="statusbar" statusbar_visible="prospet,student" />
                    </header>
                    <sheet>
                        <div class="oe_button_box" name="buttons"></div>
                    <div class="oe_inline">
                        <newline />
                        <separator string="Informations Personelles" />
                        <br />
                        <field name="photo" widget="image" class="oe_avatar oe_left" style="max-width:100px;max-height:117px;"/>
                        <div class="oe_title">
                            <group>
                                <field name="date_register" readonly="1"/>
                                <field name="name" placeholder="Noms et Prenoms"  required="1" />
                                <field name="date_nais" />
                                <field name="age"/>
                                <field name="matricule"/>
                                <field name="lieu_nais" placeholder="Lieu de Naissance"/>
                                <field name="mobility" 
                                    string="Personne a mobilité reduite ?"/>
                                <field name="nature_handicap" attrs="{'invisible': [('mobility','=', False)]}"/>
<!--                                <field name="hotel_client"/>-->
<!--                                <field name="description" attrs="{'invisible': [('hotel_client','=', False)]}"/>-->
                            </group>
                            <group>


                            </group>
                        </div>
                    </div>
                        <notebook>
                            <page string="Informations Generales">
                                <group>
                                    <group>
                                        <field name="sexe" />
                                        <field name="school"/>
                                        <field name="classe_id" domain="[('ane_academique_id.actived', '=', True)]"/>
                                        <field name="ancien_new" />
                                        <field name="occupation"/>
                                    </group>
                                    <group>
                                        <field name="ane_academique_id"/>
                                        <field name="apprenant_phone"/>
                                         <field name="religion_id"/>
                                        <field name="region_id"/>
                                    </group>
                                </group>
                            </page>
                            <page string="Informations sur les parents">
                                <group>
                                    <field name="parent_name" />
                                    <field name="parent_phone" />
                                </group>
                                 <separator string="Informations en cas d'urgence" colspan="4" />
                                    <field name="urgence_person" colspan="4" placeholder="Noms et Prenoms"/>
                                    <field name="urgence_phone" colspan="4" placeholder="Telephone"/>
                            </page>

                            <page string="Mes Payements">
                                <button name="button_generer" string="Charger les Payements"
                                        class="oe_highlight"
                                        type='object'/>
                                    <field name="payment_ids"   colspan="4">
										<tree editable="false"  create="false">
											<field name='name'/>
											<field name='matricule'/>
											<field name='ane_academique_id'/>
											<field name='date_payement'/>
                                            <field name='montant' />
                                            <field name="state"/>
										</tree>
									</field>
                            </page>

                            <page string="Pieces  Jointes">
                                <group>
                                    <field name="attachment_ids"  string="Ajouter des pieces jointes" widget="many2many_binary"/>
                                </group>
                            </page>
                            
                        </notebook>

                    </sheet>

                </form>
            </field>
        </record>

